import java.util.*;
class Employee
{
	String name;
	int empid,deptid;
	String dob;
	float sal;
	Employee(String ename,int eid,int did,String db,float s)
	{
		name=ename;
		empid=eid;
		deptid=did;
		dob=db;
		sal=s;
	}
	void show()
	{
		System.out.println("details are:"+name+empid+deptid+dob+sal);
	}
}
class Dept extends Employee
{
	String dname,add;
	int noofemp;
	Dept(String en,int ei,)
}
public class SingleInheritanceEmployee {

}
